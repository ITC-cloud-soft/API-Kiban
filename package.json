{
  "name": "aeonibs-components",
  "version": "0.0.0",
  "description": "A component library for AEON IBS websites",
  "author": "FOURDIGIT inc",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "yarn": "^1.22.10"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "eslint --color",
      "prettier --check"
    ],
    "src/**/*.scss": [
      "stylelint",
      "prettier --check"
    ]
  },
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build": "yarn bootstrap && run-s build:lib build:vanilla-examples build:ss-html build:saas build:storybook copy:dist:index",
    "build:lib": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.ts --progress",
    "build:vanilla-examples": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.vanilla-examples.ts --progress",
    "build:ss-html": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.ss-html.ts --progress",
    "build:saas": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.saas.ts --progress",
    "build:storybook": "build-storybook -o dist/storybook",
    "copy:dist:index": "copyfiles -f -V \"./src/deploy/index.html\" \"./dist\"",
    "dist": "yarn bootstrap && run-s prettier-write visual-test:update build",
    "gen:component:atom": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level atom",
    "gen:component:molecule": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level molecule",
    "gen:component:organism": "cross-env HYGEN_TMPLS=scaffolds hygen component new --level organism",
    "lint": "run-s lint:script lint:style prettier-check",
    "lint:script": "eslint ./src --ext .ts --ext .tsx --color",
    "lint:style": "stylelint 'src/**/*.scss'",
    "prettier-check": "prettier --check 'src/**/*.@(ts|tsx|scss)'",
    "prettier-check:script": "prettier --check 'src/**/*.@(ts|tsx)'",
    "prettier-check:style": "prettier --check 'src/**/*.@(scss)'",
    "prettier-write": "prettier --write 'src/**/*.@(ts|tsx|scss)'",
    "prettier-write:script": "prettier --write 'src/**/*.@(ts|tsx)'",
    "prettier-write:style": "prettier --write 'src/**/*.@(scss)'",
    "start:vanilla-examples": "yarn bootstrap && webpack-dev-server --config webpack/webpack.config.vanilla-examples.ts --progress --hot --inline",
    "start:ss-html": "yarn bootstrap && webpack-dev-server --config webpack/webpack.config.ss-html.ts --progress --hot --inline",
    "start:saas": "yarn bootstrap && webpack-dev-server --config webpack/webpack.config.saas.ts --progress --hot --inline",
    "storybook": "yarn bootstrap && start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest -c jest/config.unit-test.js",
    "test:update": "cross-env NODE_ENV=test jest -c jest/config.unit-test.js -u",
    "get-nearest-color": "ts-node-script scripts/get-nearest-color.ts",
    "visual-test": "run-s build:storybook visual-test:main",
    "visual-test:main": "cross-env NODE_ENV=test jest -c jest/config.visual-regression-test.js jest/storyshots-puppeteer.test.ts",
    "visual-test:update": "run-s build:storybook visual-test:update:main",
    "visual-test:update:main": "cross-env NODE_ENV=test jest -c jest/config.visual-regression-test.js jest/storyshots-puppeteer.test.ts -u"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
